{
  "name": "bookmark-insight",
  "version": "1.1.0",
  "type": "module",
  "description": "A powerful bookmark manager Chrome extension",
  "scripts": {
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --minify",
    "build:js": "rollup -c",
    "build": "npm run build:css && npm run build:js",
    "dev:css": "tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --watch",
    "dev": "npm run build:css && rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "rollup": "^3.25.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^4.0.0",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "chart.js": "^4.3.0",
    "dexie": "^4.2.1",
    "flexsearch": "^0.8.212",
    "mermaid": "^10.6.0"
  }
}
